        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
      </rd:DataSetInfo>
    </DataSet>
    <DataSet Name="RelatoriosDAO_FluxoFinanceiro">
      <Fields>
        <Field Name="lote">
          <DataField>lote</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="lote_pai">
          <DataField>lote_pai</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="seq_lote_pai">
          <DataField>seq_lote_pai</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="data">
          <DataField>data</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="descricao_conta">
          <DataField>descricao_conta</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cod_terceiro">
          <DataField>cod_terceiro</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="descricao_terceiro">
          <DataField>descricao_terceiro</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cod_job">
          <DataField>cod_job</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="desc_job">
          <DataField>desc_job</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="historico">
          <DataField>historico</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="deb_cred">
          <DataField>deb_cred</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="valor">
          <DataField>valor</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>strConexao</DataSourceName>
        <CommandText>select lc.lote,lc.lote_pai, lc.seq_lote_pai,lc.data,lc.cod_conta + ' - ' + cc.descricao as descricao_conta, 
                         lc.cod_terceiro,(case when lc.cod_terceiro = 0 then 'Nenhum' else ce.nome_razao_social end) as descricao_terceiro,lc.cod_job,  
                         cj.descricao + '+' + ce1.nome_fantasia as desc_job, 
                         lc.historico,lc.deb_cred, sum(lc.valor) valor 
                         from lanctos_contab lc left join cad_empresas ce on ce.cod_empresa_pai = 1 and lc.cod_terceiro = ce.cod_empresa, cad_contas cc, 
                         cad_jobs cj, cad_empresas ce1 
                         where lc.pendente='False' 
                         and lc.data &gt;= '20110115' and lc.data &lt;= '20110215'   
                         and lc.cod_empresa = 1 
                         and lc.cod_conta = cc.cod_conta 
                         and lc.cod_job = cj.cod_job 
                         and cj.cod_cliente = ce1.cod_empresa 
                         group by lc.lote,lc.lote_pai, lc.seq_lote_pai,lc.data, lc.cod_conta + ' - ' + cc.descricao,lc.cod_terceiro, 
                         (case when lc.cod_terceiro = 0 then 'Nenhum' else ce.nome_razao_social end), 
                         lc.cod_job, cj.descricao + '+' + ce1.nome_fantasia,lc.historico,lc.deb_cred</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
